{
	"name": "ondrejbrejla/eciovni",
	"description": "Component for generating invoices using Nette Framework and mPDF library.",
	"keywords": [
		"invoice",
		"generator",
		"nette",
		"latte",
		"mpdf"
	],
	"authors": [
		{
			"name": "Ondřej Brejla",
			"email": "ondrej@brejla.cz",
			"homepage": "http://brejla.cz"
		},
		{
			"name": "Jan Barášek",
			"email": "jan@barasek.com",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": ">=7.1.0",
		"nette/utils": "^3.1",
		"latte/latte": "^2.5",
		"mpdf/mpdf": "^8.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.18",
		"tracy/tracy": "^2.7",
		"phpstan/phpstan-nette": "^0.12.6"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 6 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
